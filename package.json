{
  "name": "muffs-website",
  "version": "1.0.0",
  "description": "Made using Tailwindcss",
  "main": "index.js",
  "scripts": {
    "build": "postcss ./src/styles.css -o ./public/styles.css",
    "watch": "onchange 'tailwind.config.js' './public/**/*.html' './src/**/*.css' -- npm run build",
    "prod": "NODE_ENV=production postcss ./src/styles.css -o ./public/styles.css",
    "deploy": "gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Emrikcah/muffs-website.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Emrikcah/muffs-website/issues"
  },
  "homepage": "https://github.com/Emrikcah/muffs-website#readme",
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "gh-pages": "^3.2.0",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "onchange": "^7.1.0"
  }
}
